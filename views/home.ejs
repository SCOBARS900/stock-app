<% include partials/head %>

<body> 
    <div class="container">
    <h1>Chart Stock App</h1>
    <div class="row">
      <div class="col-sm-12">  
    
          
           <form action="/newstock" method="post">
               <div class="form-group">
              <label class="category" id="stock-label">Stock:</label>
              <input type="text" class="form-control" name="stock" maxlength="50">
              <input type="submit" id="submitStock" class="form-control btn-default" value="Search">
                </div>  
           </form>
          
      <%    if (datadisplay.length > 0) { %>
      <%       var displaying = datadisplay.map(function(stock) { %>
                <div class="col-sm-6 col-md-4 col-lg-4">
                <h3><%= stock.code %></h3>
                <p><%= stock.name %></p>
                </div>
      <%       }); %>
          
      <%    }  %>
          
           
                
           
        
        
        </div>
       </div> 
    </div> 
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.3/socket.io.js"></script>
    <script src="/js/chat.js"></script>
    
    
</body>
</html>